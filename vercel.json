{
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://herbar-backend-7p73b09f8-pashas-projects-8b099908.vercel.app/api/:path*"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}